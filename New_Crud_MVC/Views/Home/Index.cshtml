@model IEnumerable<New_Crud_MVC.Models.Employee>

<ul>
    <li>@Html.ActionLink("Create","Create")</li>
</ul>
    <table class="table table-bordered">
        <tr>
            
            <th>Name</th>
            <th>Address</th>
            <th>Salary</th>
            <th>Gender</th>
            <th>Department</th>
            <th>Action</th>
            
        </tr>
        @foreach(var e in Model)
            {
    <tr>
        <td>@e.Name</td>
        <td>@e.Address</td>
        <td>@e.Salary</td>
        <td>@e.Gender</td>
        <td>@e.Department</td>
        <td>@Html.ActionLink("Edit", "Edit", new { id = e.Id }, new {@class="btn btn-primary"})</td>
        <td>@Html.ActionLink("Delete", "Delete", new { id = e.Id }, new {@class="btn btn-danger" })</td>

    </tr>
        }
    </table>